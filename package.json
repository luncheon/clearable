{
  "name": "input-clear-icon",
  "version": "0.0.2",
  "description": "Show a clear icon on the focused <input>.",
  "main": "input-clear-icon.js",
  "scripts": {
    "build": "npm run minify:css && npm run minify:js",
    "minify:css": "cleancss -o input-clear-icon.min.css input-clear-icon.css",
    "minify:js": "uglifyjs -m -c join_vars=true -o input-clear-icon.min.js input-clear-icon.js"
  },
  "repository": "luncheon/input-clear-icon",
  "author": "",
  "license": "WTFPL",
  "keywords": [],
  "devDependencies": {
    "clean-css-cli": "^4.1.10",
    "uglify-js": "^3.3.9"
  }
}
